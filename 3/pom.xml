            <!--                            </inputSpec>-->
            <!--                            <generatorName>spring</generatorName>-->
            <!--                            <apiPackage>com.example.psp.api</apiPackage>-->
            <!--                            <modelPackage>com.example.psp.model</modelPackage>-->
            <!--                            <supportingFilesToGenerate>-->
            <!--                                ApiUtil.java-->
            <!--                            </supportingFilesToGenerate>-->
            <!--                            <configOptions>-->
            <!--                                <delegatePattern>true</delegatePattern>-->
            <!--                            </configOptions>-->
            <!--                        </configuration>-->
            <!--                    </execution>-->
            <!--                </executions>-->
            <!--            </plugin>-->
        </plugins>
    </build>

</project>